-D SPI_TX_BUFSIZE=16
-D DISP_SS=PB0
-D SPI_SS_PORT=PORTB
-D SPI_SS_DDR=DDRB
-D DAC_ADDR=0xc0
-D ENC_PCIE=PCIE2
-D ENC_PCMSK=PCMSK2
-D ENC_A_PCINT=PCINT18
-D ENC_B_PCINT=PCINT19
-D ENC_PIN=PIND
-D ENC_A=PD2
-D ENC_B=PD3
-O2
-Wall
-Wextra
-std=c99
